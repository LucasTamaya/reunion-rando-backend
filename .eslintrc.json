{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "error",
    "semi": "warn",
    "quotes": "warn",
    "no-unused-vars": "warn",
    "array-callback-return": "off"
  }
}
